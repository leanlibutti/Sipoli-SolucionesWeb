


<% user = current_user%>

<% if Valorationuser.where(user_id: user.id).count == 0 %>


	<br>
	<br>
	<br>
	<br>
	<h4> No has recibido valoraciones </h4>
	<br>
	<br>
	<br>


<% else %>


	<div class="panel panel-success">
		  <div class="panel-heading">
		    <h3 class="panel-title">Valoraciones</h3>
		  </div>
		</div>


		<% Valorationuser.where(user_id: user.id).each do |valorationuser|%>



		                            <div class="panel-body">



		                            	<h3>Me valoro</h3>
		                            	<% userv = User.find(valorationuser.userValora_id)%>
		                                <h4><%= userv.name %></h4>
		                                

										<h3>Valoracion</h3>

			                             <% n = valorationuser.valor %>

			                            <% x = 1 %>

			                            <% loop do %>
			                                <% x += 1%>
			                                <%= link_to image_tag("star.png", alt: "New Think Tank"),    {:class => 'starLogo'} %>
			                                <% break if x > n %>
			                            <% end %>


		                            </div>


		<% end %>


<% end %>
